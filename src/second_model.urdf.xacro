<?xml version="1.0"?>
<robot name="arm" xmlns:xacro="http://ros.org/wiki/xacro">



  <!-- Material and Colors-->
  <material name="black">
    <color rgba="${0/255} ${0/255} ${0/255} 1.0"/>
  </material>
  <material name="dark_grey">
    <color rgba="${105/255} {105/255} ${105/255} 1.0"/>
  </material>
  <material name="grey">
    <color rgba="${128/255} ${128/255} ${128/255} 1.0"/>
  </material>
  <material name="light_grey">
    <color rgba="${221/255} ${221/255} ${221/255} 1"/>
  </material>
  <material name="white">
    <color rgba="${255/255} ${255/255} ${255/255} 1.0"/>
  </material>
  <material name="red">
    <color rgba="${255/255} ${0/255} ${0/255} 1.0"/>
